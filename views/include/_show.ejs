<% for(var i=0;i < user.length; i++){ %>
<tr>
    <td><%= i+1 %></td>
    <td><%= user[i].name %></td>
    <td><%= user[i].email %></td>
    <td><%= user[i].gender %></td>
    <td><%= user[i].status %></td>
    <td>
        <a href="/update-user?id=<%=user[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fa-solid fa-user-plus"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= user[i]._id %>>
            <span class="text-gradient"><i class="fa-solid fa-user-slash"></i></span>
        </a>
    </td>
</tr>

<% } %>